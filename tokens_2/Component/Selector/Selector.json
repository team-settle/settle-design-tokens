{
  "selector": {
    "size": {
      "large": {
        "value": "{theme.size.shape.large}",
        "type": "sizing"
      },
      "medium": {
        "value": "{theme.size.shape.medium}",
        "type": "sizing"
      },
      "small": {
        "value": "{theme.size.shape.small}",
        "type": "sizing"
      }
    },
    "spacing": {
      "small": {
        "padding": {
          "value": "{basic.size.scale.50}",
          "type": "spacing"
        }
      },
      "medium": {
        "padding": {
          "value": "{basic.size.scale.100}",
          "type": "spacing"
        }
      },
      "large": {
        "padding": {
          "value": "{basic.size.scale.150}",
          "type": "spacing"
        }
      }
    },
    "borderRadius": {
      "large": {
        "value": "{selector.size.large} * {basic.borderRadius.round}",
        "type": "borderRadius"
      },
      "medium": {
        "value": "{selector.size.medium}* {basic.borderRadius.round}",
        "type": "borderRadius"
      },
      "small": {
        "value": "{selector.size.small} * {basic.borderRadius.round}",
        "type": "borderRadius"
      }
    },
    "color": {
      "unselected": {
        "default": {
          "body": {
            "type": "color",
            "value": "{theme.color.common.onSurfaceSecondary}"
          },
          "highlight": {
            "type": "color",
            "value": "{theme.color.common.none}"
          }
        },
        "hovered": {
          "body": {
            "type": "color",
            "value": "{theme.color.common.onSurfaceSecondary}"
          },
          "highlight": {
            "value": "rgba({theme.color.common.onSurfaceOpaque}, {theme.opacity.state.hovered})",
            "type": "color"
          }
        },
        "disabled": {
          "body": {
            "value": "rgba({theme.color.common.onSurfaceOpaque}, {basic.opacity.200})",
            "type": "color"
          },
          "highlight": {
            "type": "color",
            "value": "{theme.color.common.none}"
          }
        }
      },
      "selected": {
        "default": {
          "body": {
            "type": "color",
            "value": "{theme.color.common.primary}"
          },
          "highlight": {
            "type": "color",
            "value": "{theme.color.common.none}"
          }
        },
        "hovered": {
          "body": {
            "type": "color",
            "value": "{theme.color.common.primary}"
          },
          "highlight": {
            "value": "rgba({theme.color.common._primary}, {theme.opacity.state.hovered})",
            "type": "color"
          }
        },
        "disabled": {
          "body": {
            "value": "rgba({theme.color.common.onSurfaceOpaque}, {basic.opacity.200})",
            "type": "color"
          },
          "highlight": {
            "type": "color",
            "value": "{theme.color.common.none}"
          }
        }
      },
      "indeterminate": {
        "default": {
          "body": {
            "type": "color",
            "value": "{theme.color.common.primary}"
          },
          "highlight": {
            "type": "color",
            "value": "{theme.color.common.none}"
          }
        },
        "hovered": {
          "body": {
            "type": "color",
            "value": "{theme.color.common.primary}"
          },
          "highlight": {
            "value": "rgba({theme.color.common._primary}, {theme.opacity.state.hovered})",
            "type": "color"
          }
        },
        "disabled": {
          "body": {
            "value": "rgba({theme.color.common.onSurfaceOpaque}, {basic.opacity.200})",
            "type": "color"
          },
          "highlight": {
            "type": "color",
            "value": "{theme.color.common.none}"
          }
        }
      }
    }
  }
}