{
  "banner": {
    "size": {
      "height": {
        "value": "{theme.size.shape.xlarge}",
        "type": "sizing"
      }
    },
    "spacing": {
      "padding": {
        "value": "{basic.size.scale.150} {basic.size.scale.300}",
        "type": "spacing"
      },
      "text": {
        "value": "{basic.size.scale.75} {basic.size.scale.none}",
        "type": "spacing"
      },
      "gap": {
        "value": "{basic.size.scale.150}",
        "type": "spacing"
      }
    },
    "borderWidth": {
      "info": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "caution": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "alert": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "success": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      }
    },
    "color": {
      "info": {
        "background": {
          "value": "$theme.color.common.infoContainer",
          "type": "color"
        },
        "onBackground": {
          "value": "$theme.color.common.info",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      },
      "warning": {
        "background": {
          "value": "{theme.color.common.warningContainer}",
          "type": "color"
        },
        "onBackground": {
          "value": "{theme.color.common.warning}",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      },
      "error": {
        "background": {
          "value": "$theme.color.common.errorContainer",
          "type": "color"
        },
        "onBackground": {
          "value": "$theme.color.common.error",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      },
      "success": {
        "background": {
          "value": "$theme.color.common.successContainer",
          "type": "color"
        },
        "onBackground": {
          "value": "$theme.color.common.success",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      }
    }
  }
}