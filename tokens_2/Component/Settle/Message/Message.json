{
  "message": {
    "borderRadius": {
      "medium": {
        "value": "{message.size.height} * {basic.borderRadius.small}",
        "type": "borderRadius"
      }
    },
    "spacing": {
      "spacing": {
        "value": "{basic.size.scale.100}",
        "type": "spacing"
      },
      "text": {
        "value": "{basic.size.scale.25} {basic.size.scale.50}",
        "type": "spacing"
      },
      "gap": {
        "value": "{basic.size.scale.50}",
        "type": "spacing"
      }
    },
    "borderWidth": {
      "info": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "caution": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "alert": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "success": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      }
    },
    "color": {
      "info": {
        "background": {
          "value": "$theme.color.common.infoContainer",
          "type": "color"
        },
        "onBackground": {
          "value": "$theme.color.common.info",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      },
      "warning": {
        "background": {
          "value": "{theme.color.common.warningContainer}",
          "type": "color"
        },
        "onBackground": {
          "value": "{theme.color.common.warning}",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      },
      "error": {
        "background": {
          "value": "$theme.color.common.errorContainer",
          "type": "color"
        },
        "onBackground": {
          "value": "$theme.color.common.error",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      },
      "success": {
        "background": {
          "value": "$theme.color.common.successContainer",
          "type": "color"
        },
        "onBackground": {
          "value": "$theme.color.common.success",
          "type": "color"
        },
        "outline": {
          "value": "$basic.color.base.none",
          "type": "color"
        }
      }
    },
    "size": {
      "height": {
        "value": "{theme.size.shape.medium}",
        "type": "sizing"
      }
    }
  }
}