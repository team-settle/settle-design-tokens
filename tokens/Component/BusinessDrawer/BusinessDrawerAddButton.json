{
  "businessDrawerAddButton": {
    "color": {
      "default": {
        "background": {
          "type": "color",
          "value": "{theme.color.common.none}"
        },
        "text": {
          "value": "rgba({theme.color.common.onPrimary}, {basic.opacity.600})",
          "type": "color"
        },
        "pictogram-outline": {
          "type": "color",
          "value": "{theme.color.common.none}"
        },
        "pictogram-background": {
          "value": "rgba({theme.color.common.onPrimary}, {basic.opacity.200})",
          "type": "color"
        }
      },
      "hovered": {
        "background": {
          "value": "rgba({theme.color.common.onPrimary}, {basic.opacity.200})",
          "type": "color"
        },
        "text": {
          "type": "color",
          "value": "{theme.color.common.onPrimary}"
        },
        "pictogram-outline": {
          "type": "color",
          "value": "{theme.color.common.none}"
        },
        "pictogram-background": {
          "value": "rgba({theme.color.common.onPrimary}, {basic.opacity.200})",
          "type": "color"
        }
      },
      "selected": {
        "background": {
          "type": "color",
          "value": "{theme.color.common.none}"
        },
        "text": {
          "type": "color",
          "value": "{theme.color.common.onPrimary}"
        },
        "pictogram-outline": {
          "type": "color",
          "value": "{theme.color.common.onPrimary}"
        },
        "pictogram-background": {
          "value": "rgba({theme.color.common.onPrimary}, {basic.opacity.200})",
          "type": "color"
        }
      }
    },
    "borderWidth": {
      "pictogram": {
        "default": {
          "value": "{basic.borderWidth.200}",
          "type": "borderWidth"
        },
        "hovered": {
          "value": "{basic.borderWidth.200}",
          "type": "borderWidth"
        },
        "selected": {
          "value": "{basic.borderWidth.200}",
          "type": "borderWidth"
        }
      },
      "container": {
        "default": {
          "value": "{basic.borderWidth.none}",
          "type": "borderWidth"
        },
        "hovered": {
          "value": "{basic.borderWidth.none}",
          "type": "borderWidth"
        },
        "selected": {
          "value": "{basic.borderWidth.none}",
          "type": "borderWidth"
        }
      }
    },
    "size": {
      "pictogram": {
        "desktop": {
          "size": {
            "value": "{theme.size.shape.desktop.medium}",
            "type": "sizing"
          }
        },
        "mobile": {
          "size": {
            "value": "{theme.size.shape.mobile.medium}",
            "type": "sizing"
          }
        }
      },
      "container": {
        "desktop": {
          "height": {
            "value": "{theme.size.shape.desktop.xlarge}",
            "type": "sizing"
          }
        },
        "mobile": {
          "height": {
            "value": "{theme.size.shape.mobile.xlarge}",
            "type": "sizing"
          }
        }
      }
    },
    "borderRadius": {
      "pictogram": {
        "desktop": {
          "value": "{basic.borderRadius.round} * {profileDrawerAddButton.size.pictogram.desktop.size}",
          "type": "borderRadius"
        },
        "mobile": {
          "value": "{basic.borderRadius.round} * {profileDrawerAddButton.size.pictogram.mobile.size}",
          "type": "borderRadius"
        }
      },
      "container": {
        "desktop": {
          "value": "{theme.borderRadius.desktop.none}",
          "type": "borderRadius"
        },
        "mobile": {
          "value": "{theme.borderRadius.mobile.none}",
          "type": "borderRadius"
        }
      }
    }
  }
}