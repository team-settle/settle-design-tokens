{
  "container": {
    "borderRadius": {
      "desktop": {
        "value": "$theme.borderRadius.desktop.medium",
        "type": "borderRadius"
      },
      "mobile": {
        "value": "$theme.borderRadius.mobile.none",
        "type": "borderRadius"
      }
    },
    "borderWidth": {
      "desktop": {
        "value": "$basic.borderWidth.100",
        "type": "borderWidth"
      },
      "mobile": {
        "value": "$basic.borderWidth.none",
        "type": "borderWidth"
      }
    },
    "color": {
      "default": {
        "background": {
          "value": "$theme.color.common.surface",
          "type": "color"
        },
        "outline": {
          "value": "$theme.color.common.outline",
          "type": "color"
        }
      }
    },
    "space": {
      "desktop": {
        "margin-left": {
          "value": "$basic.size.scale.desktop.none",
          "type": "spacing"
        },
        "margin-right": {
          "value": "$basic.size.scale.desktop.300",
          "type": "spacing"
        },
        "margin-top": {
          "value": "$basic.size.scale.desktop.none",
          "type": "spacing"
        },
        "margin-bottom": {
          "value": "$basic.size.scale.desktop.300",
          "type": "spacing"
        }
      },
      "mobile": {
        "margin-left": {
          "value": "$basic.size.scale.desktop.none",
          "type": "spacing"
        },
        "margin-right": {
          "value": "$basic.size.scale.mobile.none",
          "type": "spacing"
        },
        "margin-top": {
          "value": "$basic.size.scale.desktop.none",
          "type": "spacing"
        },
        "margin-bottom": {
          "value": "$basic.size.scale.mobile.none",
          "type": "spacing"
        }
      }
    }
  }
}