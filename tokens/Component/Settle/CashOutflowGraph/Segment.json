{
  "cashOutflowGraphSegment": {
    "color": {
      "expected-bill": {
        "background": {
          "value": "{theme.color.common.surface}",
          "type": "color"
        },
        "outline": {
          "value": "{basic.color.grey.300}",
          "type": "color"
        }
      },
      "expected-po": {
        "background": {
          "value": "{basic.color.grey.300}",
          "type": "color"
        },
        "outline": {
          "value": "{cashOutflowGraphSegment.color.expected-po.background}",
          "type": "color"
        }
      },
      "pending": {
        "background": {
          "value": "{basic.color.indigo.100}",
          "type": "color"
        },
        "outline": {
          "value": "{cashOutflowGraphSegment.color.pending.background}",
          "type": "color"
        }
      },
      "failed": {
        "background": {
          "value": "{basic.color.red.200}",
          "type": "color"
        },
        "outline": {
          "value": "{cashOutflowGraphSegment.color.failed.background}",
          "type": "color"
        }
      },
      "interest": {
        "backgroud": {
          "value": "{theme.color.common.secondary}",
          "type": "color",
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": ".4",
                "space": "lch"
              }
            }
          }
        },
        "outline": {
          "value": "{cashOutflowGraphSegment.color.interest.backgroud}",
          "type": "color"
        }
      },
      "repayment": {
        "background": {
          "value": "{theme.color.common.secondary}",
          "type": "color"
        },
        "outline": {
          "value": "{cashOutflowGraphSegment.color.repayment.background}",
          "type": "color"
        }
      },
      "cashpayment": {
        "background": {
          "value": "{basic.color.bluegrey.300}",
          "type": "color"
        },
        "outline": {
          "value": "{cashOutflowGraphSegment.color.cashpayment.background}",
          "type": "color"
        }
      }
    },
    "borderRadius": {
      "value": "$theme.borderRadius.desktop.small",
      "type": "borderRadius"
    }
  }
}