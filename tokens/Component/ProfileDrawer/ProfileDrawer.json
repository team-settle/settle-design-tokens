{
  "profileDrawer": {
    "color": {
      "background": {
        "type": "color",
        "value": "{theme.color.common.primary}"
      }
    },
    "spacing": {
      "desktop": {
        "padding-h": {
          "value": "$basic.size.scale.desktop.none",
          "type": "spacing"
        },
        "padding-v": {
          "value": "$basic.size.scale.desktop.100",
          "type": "spacing"
        }
      },
      "mobile": {
        "padding-h": {
          "value": "$basic.size.scale.mobile.none",
          "type": "spacing"
        },
        "padding-v": {
          "value": "$basic.size.scale.mobile.100",
          "type": "spacing"
        }
      }
    }
  }
}