{
  "selector": {
    "size": {
      "desktop": {
        "large": {
          "value": "{theme.size.shape.desktop.large}",
          "type": "sizing"
        },
        "medium": {
          "value": "{theme.size.shape.desktop.medium}",
          "type": "sizing"
        },
        "small": {
          "value": "{theme.size.shape.desktop.small}",
          "type": "sizing"
        }
      },
      "mobile": {
        "large": {
          "value": "{theme.size.shape.mobile.large}",
          "type": "sizing"
        },
        "medium": {
          "value": "{theme.size.shape.mobile.medium}",
          "type": "sizing"
        },
        "small": {
          "value": "{theme.size.shape.mobile.small}",
          "type": "sizing"
        }
      }
    },
    "borderRadius": {
      "desktop": {
        "large": {
          "value": "$selector.size.desktop.large * {basic.borderRadius.round}",
          "type": "borderRadius"
        },
        "medium": {
          "value": "$selector.size.desktop.medium* {basic.borderRadius.round}",
          "type": "borderRadius"
        },
        "small": {
          "value": "$selector.size.desktop.small * {basic.borderRadius.round}",
          "type": "borderRadius"
        }
      },
      "mobile": {
        "large": {
          "value": "$selector.size.mobile.large * {basic.borderRadius.round}",
          "type": "borderRadius"
        },
        "medium": {
          "value": "$selector.size.mobile.medium * {basic.borderRadius.round}",
          "type": "borderRadius"
        },
        "small": {
          "value": "$selector.size.mobile.small* {basic.borderRadius.round}",
          "type": "borderRadius"
        }
      }
    },
    "color": {
      "unselected-default": {
        "body": {
          "type": "color",
          "value": "{theme.color.common.onSurfaceSecondary}"
        },
        "highlight": {
          "type": "color",
          "value": "{theme.color.common.none}"
        }
      },
      "unselected-hovered": {
        "body": {
          "type": "color",
          "value": "{theme.color.common.onSurfaceSecondary}"
        },
        "highlight": {
          "value": "rgba({theme.color.common.onSurfaceOpaque}, {theme.opacity.state.hovered})",
          "type": "color"
        }
      },
      "unselected-disabled": {
        "body": {
          "value": "rgba({theme.color.common.onSurfaceOpaque}, {basic.opacity.200})",
          "type": "color"
        },
        "highlight": {
          "type": "color",
          "value": "{theme.color.common.none}"
        }
      },
      "selected-default": {
        "body": {
          "type": "color",
          "value": "{theme.color.common.secondary}"
        },
        "highlight": {
          "type": "color",
          "value": "{theme.color.common.none}"
        }
      },
      "selected-hovered": {
        "body": {
          "type": "color",
          "value": "{theme.color.common.secondary}"
        },
        "highlight": {
          "value": "rgba({theme.color.common.primary}, {theme.opacity.state.hovered})",
          "type": "color"
        }
      },
      "selected-disabled": {
        "body": {
          "value": "rgba({theme.color.common.onSurfaceOpaque}, {basic.opacity.200})",
          "type": "color"
        },
        "highlight": {
          "type": "color",
          "value": "{theme.color.common.none}"
        }
      },
      "indeterminate-default": {
        "body": {
          "type": "color",
          "value": "{theme.color.common.secondary}"
        },
        "highlight": {
          "type": "color",
          "value": "{theme.color.common.none}"
        }
      },
      "indeterminate-hovered": {
        "body": {
          "type": "color",
          "value": "{theme.color.common.secondary}"
        },
        "highlight": {
          "value": "rgba({theme.color.common.primary}, {theme.opacity.state.hovered})",
          "type": "color"
        }
      },
      "indeterminate-disabled": {
        "body": {
          "value": "rgba({theme.color.common.onSurfaceOpaque}, {basic.opacity.200})",
          "type": "color"
        },
        "highlight": {
          "type": "color",
          "value": "{theme.color.common.none}"
        }
      }
    }
  }
}