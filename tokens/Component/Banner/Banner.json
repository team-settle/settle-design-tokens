    "size": {
      "desktop": {
        "value": "{theme.size.shape.desktop.large}",
        "type": "sizing"
      },
      "mobile": {
        "value": "{theme.size.shape.mobile.large}",
        "type": "sizing"
      }
    },
    "spacing": {
      "desktop": {
        "horizontal": {
          "value": "$basic.size.scale.desktop.300",
          "type": "spacing"
        },
        "vertical": {
          "value": "$basic.size.scale.desktop.150",
          "type": "spacing"
        },
        "gap": {
          "value": "$basic.size.scale.desktop.150",
          "type": "spacing"
        }
      },
      "mobile": {
        "horizontal": {
          "value": "$basic.size.scale.mobile.300",
          "type": "spacing"
        },
        "vertical": {
          "value": "$basic.size.scale.mobile.150",
          "type": "spacing"
        },
        "gap": {
          "value": "$basic.size.scale.mobile.150",
          "type": "spacing"
        }
      }
    },
    "borderWidth": {
      "info": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "warning": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "error": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      },
      "success": {
        "value": "{basic.borderWidth.none}",
        "type": "borderWidth"
      }
    },
    "color": {
