{
  "trailItem": {
    "timeline": {
      "color": {
        "line": {
          "type": "color",
          "value": "{theme.color.common.separator}"
        }
      },
      "width": {
        "value": "{basic.borderWidth.200}",
        "type": "borderWidth"
      },
      "spacing": {
        "desktop": {
          "left": {
            "value": "($rowWithSlot.slot.size.desktop / 2) - ($trailItem.timeline.width / 2)",
            "type": "spacing"
          },
          "top": {
            "value": "$basic.size.scale.desktop.350",
            "type": "spacing"
          }
        },
        "mobile": {
          "left": {
            "value": "($rowWithSlot.slot.size.mobile / 2) - ($trailItem.timeline.width / 2)",
            "type": "spacing"
          },
          "top": {
            "value": "$basic.size.scale.mobile.350",
            "type": "spacing"
          }
        }
      }
    }
  }
}